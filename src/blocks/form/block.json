{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "gutenberg-form/form",
	"apiVersion": 3,
	"title": "Form",
	"description": "Add a form as Block to a page.",
	"textdomain": "gutenberg-form",
	"category": "widgets",
	"attributes": {
		"formPost": {
			"type": "number",
			"default": 0
		}
	},
	"usesContext": ["postType"],
	"render": "file:./render.php",
	"editorStyle": "gbf-form-editor-style",
	"editorScript": "gbf-form-editor"
}
