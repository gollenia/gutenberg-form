{
	"name": "gutenberg-form/submit",
	"apiVersion": 2,
	"title": "Submut",
	"description": "Lets the user type a short text",
	"category": "widgets",
	"attributes": {
		"name": {
			"type": "string",
			"default": "submit"
		},
		"width": {
			"type": "integer",
			"default": 6
		},
		"alignment": {
			"type": "right",
			"default": ""
		},
		"label": {
			"type": "string",
			"default": ""
		},
		"buttonBackgroundColor": {
			"type": "string",
			"default": "primary"
		},
		"buttonTextColor": {
			"type": "string",
			"default": "white"
		},
		"customButtonBackgroundColor": {
			"type": "string",
			"default": ""
		},
		"customButtonTextColor": {
			"type": "string",
			"default": ""
		},
		"style": {
			"type": "object",
			"default": {
				"spacing": {
					"padding": {
						"top": "0.6rem",
						"right": "1rem",
						"bottom": "0.6rem",
						"left": "1rem"
					}
				}
			}
		}
	},
	"parent": ["gutenberg-form/form-fields"],
	"supports": {
		"html": false,
		"multiple": false,
		"injector": true,
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": false,
			"width": false,
			"__experimentalDefaultControls": {
				"radius": true,
				"style": false,
				"width": true,
				"linked": false,
				"color": true
			}
		},
		"spacing": {
			"padding": true,
			"margin": true
		}
	},
	"styles": [
		{
			"name": "fill",
			"label": "Fill",
			"isDefault": true
		},
		{
			"name": "outline",
			"label": "Outline"
		}
	]
}
